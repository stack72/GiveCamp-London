<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
<head>
    <title>@ViewBag.Title</title>
    <link rel="icon" href="@Url.Content("~/content/images/favicon.ico")" type="image/x-icon" />
    <link rel="shortcut icon" href="@Url.Content("~/content/images/favicon.ico")" type="image/vnd.microsoft.icon" />
    <link rel="stylesheet" href="@Url.Content("~/content/template.css")" type="text/css" media="screen" />
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.showoff.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" href="@Url.Content("~/content/jshowoff.css")" type="text/css" />
    @RenderSection("headInclude", false)
    @Html.Partial("_GoogleAnalytics");
</head>
<body>
    <div id="wrapper">
        <div id="header" class="clearfix">
            <div id="logo">
                <a href="/">Give Camp UK</a></div>
            <div id="header-items" class="clearfix">
                @Html.Partial("_SocialChannels")
                <div id="breadcrumbs" class="clear clearfix">
                </div>
            </div>
            @Html.Action("EventFullBanner", "Content")
        </div>
        @Html.Partial("_MainNavigation")
        <div id="column-right-540">
            <div class="article">
                @RenderBody()
            </div>
        </div>
        <div id="column-left-360">
            <div class="content-module">
                <h2>GIVECAMP UK: A PART OF INTERNATIONAL GIVECAMP</h2>
                <div class="content">
                    <ul style="color: white">
                        <li><span style="font-weight: bolder">Date:</span> 21st to 23rd October 2011</li>
                        <li><span style="font-weight: bolder">Location:</span> UCL Bloomsbury Campus<br />
                            Gower Street<br />
                            London<br />
                            WC1E 6BT</li>
                    </ul>
                </div>
            </div>
            <div class="content-module">
                <h2>Sponsors</h2>
                @Html.Action("RotatorContent", "Content")
            </div>
            <div class="content-module">
                @Html.Partial("_Contributors")
            </div>
        </div>
        @Html.Partial("_Footer")
    </div>
</body>
</html>
